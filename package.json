{
  "name": "@eresearchqut/awx-api-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eresearchqut/awx-api-client.git"
  },
  "version": "1.0.0",
  "license": "MIT",
  "release": {
    "branches": [
      "main"
    ],
    "repositoryUrl": "https://github.com/eresearchqut/awx-api-client.git"
  },
  "scripts": {
    "awx:build": "(git -C awx pull || git clone -b devel https://github.com/ansible/awx.git awx) && (cd awx && make docker-compose-build && make clean/ui-next ui-next)",
    "awx:start": "(cd awx && make docker-compose)",
    "awx:superuser": "docker exec -ti tools_awx_1 awx-manage createsuperuser",
    "clean": "rimraf awx && rimraf dist && rimraf generated && rimraf yarn-error.log && rimraf node_modules",
    "regen": "(cd awx && make docker-compose)",
    "build": "node ./generate.js",
    "postbuild": "(cd ./generated/typescript-fetch && yarn install && yarn build)",
    "test": "tsc && jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@eresearchqut/jest-testcontainers": "^3.4.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "node-fetch": "^2.6.13",
    "typescript": "^5.5.3",
    "dotenv": "^16.3.1",
    "rimraf": "^5.0.8"
  }
}
